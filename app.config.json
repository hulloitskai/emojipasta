{
  "expo": {
    "name": "Emojipasta",
    "slug": "emojipasta",
    "description": "An app for skimming through r/emojipasta.",
    "version": "0.1.1",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 30
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "buildNumber": "2",
      "bundleIdentifier": "travel.joya.emojipasta",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 2,
      "package": "travel.joya.emojipasta"
    }
  }
}
